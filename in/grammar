, +esz 		MATCH: [^sz]	IF: quasi_analytic_V_indic synthetic_inflex front
, +ol 		MATCH: [sz]	IF: synthetic_inflex back
, +el		MATCH: [sz]	IF: synthetic_inflex front non_round
, +öl 		MATCH: [sz]	IF: synthetic_inflex front round

