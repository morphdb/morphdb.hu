PP_APPENDIX
	PASS: true
, IF: refl_pron 
  FREE: false 
  OUT: NOUN
  OUT: tagged defective_derivation
  OUT: low non_bareT
  OUT: defective_inflection sing anp case non_poss
, IF: pp_appendix 
  KEEP: pp_re pp_ról pp_nál pp_ig front back
  OUT: PP_RE PP_RÓL PP_IG PP_NÁL
  FREE: false
, IF: !pp_appendix !refl_pron
  KEEP: pp_re pp_ról pp_nál pp_ig front back
  OUT: PP_RE PP_RÓL PP_IG PP_NÁL
;

