, +juk	IF: back analytic poss_sg_jA	OUT: back	#pl.: holdjuk, napjuk
, +jük	IF: front analytic poss_sg_jA	OUT: front round	#pl.: cseppjük, kertjük
, +uk	IF: back synthetic_noun poss_sg_A	OUT: back	#pl.: házuk, mondatuk
