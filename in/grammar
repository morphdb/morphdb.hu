NOM_STEM_FINAL_LENGTHENING
	FREE: false
	PASS: true
	FILTER: lengthened non_lengthened
	OUT: NOM_KEEP_ALL_FEATURES
	OUT: NOM_PARADIGM 
	OUT: V_final 
	
, +á IF: back !hyphen MATCH: a$   CLIP: 1   OUT: lengthened 
, +é IF: front non_round !hyphen CLIP: 1 MATCH: e$    OUT: lengthened 
, +ó IF: back	!hyphen CLIP: 1 MATCH: o$	OUT: lengthened 
, +õ IF: front round	 !hyphen CLIP: 1	MATCH: ö$	OUT: lengthened 
  , MATCH: [aeoö]$        	 OUT:  non_lengthened
  # hack for poss checking
, +i MATCH: i IF: !hyphen CLIP: 1  	    OUT: lengthened non_lengthened 
, +á- IF: back hyphen CLIP: 1 MATCH: a$   OUT: lengthened 
, +é- IF: front non_round hyphen CLIP: 1 MATCH: e$    OUT: lengthened 
, +ó- IF: back hyphen 	CLIP: 1 MATCH: o$	OUT: lengthened 
, +õ- IF: front round hyphen	CLIP: 1 MATCH: ö$	OUT: lengthened 
, +-  IF: hyphen MATCH: [aeoö]$         	 OUT:  non_lengthened
  # hack for poss checking
, +i- IF: hyphen MATCH: i CLIP: 1 OUT: lengthened non_lengthened 
  , MATCH: [^aeoöi<C>]$          OUT: lengthened non_lengthened     

;

#NOM_PROPER_NOUN_FILTER
#	OUT: NOM_KEEP_ALL_FEATURES
#	OUT: NOM_PARADIGM
#	FREE: false
#,	IF: proper_noun non_lengthened
#,	IF: !proper_noun non_lengthened COMPOUND_CAT: NOM
#
#,	IF: lengthened
#;




#*************
#* MORPHEMES *
#*************

SING
	FREE: false
	PASS: true
	OUT: NOM_KEEP_ALL_FEATURES
,	OUT: CASE	
	IF: sing non_poss
,	IF: analytic non_lengthened !proper_noun non_poss !possessed
	COMPOUND_MOD: NOM
,	+- IF: analytic non_lengthened !hyphen !proper_noun non_poss !possessed
	COMPOUND_MOD: NOM
;


PLUR 
	FREE: false
	TAG: <PLUR>
	FS: <PLUR>
	IF: plur non_poss
	IF: synthetic_noun lengthened       
	OUT: ANP CASE case low lengthened non_lengthened analytic non_bareT synthetic_noun C_final k_final
	OUT: non_poss anp superessive

, +k	IF: non_low V_final 	KEEP: back front round non_round	#pl.: hajók, büfék, teknõk
, +ak	IF: low back	OUT: back	#pl.: házak
, +ek	IF: low front	OUT: front non_round	#pl.: elvek, fülek
, +ak	MATCH: ó #IF: adj  	OUT: back	#pl.: hosszúak, állandóak
, +ek	IF: adj V_final front round OUT: front non_round	#pl.: hûek, egyenlõek 
, +ok	IF: C_final	IF: non_low back	OUT: back	#pl.: mondatok
, +ek	IF: C_final    IF: non_low front non_round	OUT: front non_round	#pl.: életek
, +ök	IF: C_final    IF: non_low front round	OUT: front round	#pl.: gyökök
;                           
                                                 
FAM 
	FREE: false	
	IF: fam
	IF: analytic lengthened 
	OUT: ANP CASE case low non_round lengthened non_lengthened analytic non_bareT synthetic_noun non_poss anp superessive C_final k_final
	KEEP: front back 
	TAG: <PLUR<FAM>>

# mókusék, Enikõék
, +ék	

;

POSS_SG_1 
	FREE: false	
	IF: poss sing
	IF: synthetic_noun lengthened
	OUT: FAM ANP CASE case low lengthened non_lengthened analytic non_bareT synthetic_noun non_poss anp fam superessive C_final m_final
	TAG: <POSS<1>>

, +m	IF: V_final	KEEP: back front round non_round	#pl.: hajóm, büfém, teknõm
, +am	IF: C_final	IF: low back	OUT: back	#pl.: házam
, +em	IF: C_final	IF: low front	OUT: front non_round	#pl.: elvem, fülem
, +om	IF: C_final	IF: non_low back	OUT: back	#pl.: mondatom
, +em	IF: C_final	IF: non_low front non_round	OUT: front non_round	#pl.: életem
, +öm	IF: C_final	IF: non_low front round	OUT: front round	#pl.: gyököm

;

POSS_SG_2
	FREE: false	
	IF: poss sing
	IF: synthetic_noun lengthened
	OUT: FAM ANP CASE case low lengthened non_lengthened analytic non_bareT synthetic_noun non_poss anp fam superessive C_final d_final
	TAG: <POSS<2>>

, +d	IF: V_final	KEEP: back front round non_round	#pl.: hajód, büféd, teknõd
, +ad	IF: C_final	IF: low back	OUT: back	#pl.: házad
, +ed	IF: C_final	IF: low front	OUT: front non_round	#pl.: elved, füled
, +od	IF: C_final	IF: non_low back	OUT: back	#pl.: mondatod
, +ed	IF: C_final 	IF: non_low front non_round	OUT: front non_round	#pl.: életed
, +öd	IF: C_final	IF: non_low front round	OUT: front round	#pl.: gyököd
;

POSS_SG_3
	FREE: false
	IF: poss sing
	IF: lengthened
	TAG: <POSS>
	OUT: NOM_STEM_FINAL_LENGTHENING 
	OUT: case analytic synthetic_noun non_poss anp sing fam superessive bareT 
	V_final non_low possessed

, +ja	IF: back analytic poss_sg_jA	OUT: back non_low	#pl.: holdja, napja
, +je	IF: front analytic poss_sg_jA	OUT: front non_round non_low	#pl.: cseppje, kertje
, +a	IF: back synthetic_noun poss_sg_A	OUT: back non_low	#pl.: háza, mondata
, +e	IF: front synthetic_noun poss_sg_A	OUT: front non_round non_low	#pl.: elve, füle

;

